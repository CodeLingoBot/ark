#use std::mem
#use std::io
#use std::strings

pub func main() -> int {
	filePath := c"../CONTRIBUTING.md";
	maybeFile: Option<^mut io::File> = io::File::open(filePath, io::FileMode::Read);

	mut myFile: ^mut io::File;

	match maybeFile {
        Some(t) => myFile = t,
        None => {
        	io::println("Error! Failed to read file");
        	return -1;
        },
    }

    result := myFile.readToString();
    match result {
    	Some(s) => {
    		io::printDynamicString(s);
    	},
    	None => {
    		io::println("Failed to print file contents!");
    		return -1;
    	},
    }

	myFile.destroy();

	return 0;
}