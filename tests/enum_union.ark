[c] func printf(fmt: ^u8, ...) -> int;

type Tree enum {
    Node{left: ^Tree, right: ^Tree},
    Leaf(int)
};

func main() -> int {
	x := Tree::Leaf(42);
	y := Tree::Leaf(36);
	z := Tree::Node{left: ^Tree(&x), right: ^Tree(&y)};

    C::printf(c"Leaf-tag: %d\n", x);
    C::printf(c"Leaf-tag: %d\n", y);
    C::printf(c"Node-tag: %d\n", z);
    return 0;
}
