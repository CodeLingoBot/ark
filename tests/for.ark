[c] func printf(fmt: str, ...) -> int;

func main() -> int {
    mut i := 0;

    for i < 5 {
        C::printf("i: %d\n", i);
        i = i + 1;
    }

    mut x := 0;

    for x < 10 {
        C::printf("x: %d\n", x);

        x += 1;

        for {
            break;
        }

        if x == 5 {
            break;
        } else {
            C::printf("else\n");
        }
    }

    mut y := 0;
    for y <= 100 {
        C::printf("y: %d\n", y);
        y += 1;
        if y < 5 {
            next;
        } else {
            break;
        }
    }

    return 0;
}
