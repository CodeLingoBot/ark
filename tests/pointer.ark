[c] func printf(fmt: ^u8, ...) -> int;

func swap(mut a: ^s32, mut b: ^s32)
{
   mut temp: s32 = ^a;
   ^a = ^b;
   ^b = temp;
}

pub func main() -> int
{
    a: s32 = 5;
    b: s32 = 6;
    C::printf(c"a: %d, b: %d\n", a, b);
    swap(^s32(&a), ^s32(&b));
    C::printf(c"a: %d, b: %d\n", a, b);

    return 6 - int(a); // Inference problems ugh
}
