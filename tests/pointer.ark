[c] func printf(fmt: ^u8, ...) -> int;

func swap<T>(a: ^mut T, b: ^mut T)
{
	(@a, @b) = (@b, @a);
}

pub func main() -> int
{
    a: s32 = 5;
    b: s32 = 6;
    C::printf(c"a: %d, b: %d\n", a, b);
    swap(^mut a, ^mut b);
    C::printf(c"a: %d, b: %d\n", a, b);

    return 6 - int(a); // Not inference problems ugh
}
