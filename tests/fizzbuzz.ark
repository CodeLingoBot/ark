// Fizzbuzz
// %3 -> Fizz
// %5 -> Buzz

[c] func printf(fmt: ^u8, ...) -> int;
[c] func putchar(c: s8);

pub func main() -> int {
    mut i := 1;

    for i <= 100 {
        div3 := i % 3 == 0;
        div5 := i % 5 == 0;

        if div3 {
            C::printf(c"Fizz");
        }

        if div5 {
            C::printf(c"Buzz");
        }

        if !(div3 || div5) {
            C::printf(c"%d", i);
        }

        C::putchar(s8('\n'));

        i += 1;
    }

    return 0;
}
