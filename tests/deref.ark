[c] func printf(fmt: ^u8, ...) -> int;

pub func main() -> int {
    mut x: s32 = 9;
    y: ^s32 = ^x;

    C::printf(c"%d\n", @y);

    x = 15;
    z: ^^s32 = ^y;

    C::printf(c"%d\n", @@z);

    x = 9000;

    a: ^^^s32 = ^z;
    b: ^^^^s32 = ^a;
    c: ^^^^^s32 = ^b;
    d: ^^^^^^s32 = ^c;
    e: ^^^^^^^s32 = ^d;

    C::printf(c"%d\n", @@@@@@@e);

    return 0;
}
