[c] func printf(fmt: ^u8, ...) -> int;

func do(thing: int) {
    C::printf("%d\n", thing);
}

func main() -> int {
    C::printf("Func ptr test\n");

    fn := func() -> int {
        return 5;
    };

    another := func() -> int => return 6;

    mut dofn: func(int);
    dofn = do;

    [unused] dofn2 := do;

    dofn(fn());
    dofn(another());

    runFunc(func() {
        C::printf("Printing from a lambda!\n");
    });

    return 0;
}

func runFunc(fn: func()) {
    fn();
}
